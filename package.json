{
  "name": "cockpit",
  "version": "0.0.1",
  "private": true,
  "author": "Caibrain labs",
  "license": "GPL-3.0",
  "homepage": "https://github.com/Aimosphere-Network/cockpit",
  "bugs": {
    "url": "https://github.com/Aimosphere-Network/cockpit/issues"
  },
  "keywords": [
    "aimosphere",
    "aimosphere-ui",
    "cockpit"
  ],
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "CI=true react-app-rewired test",
    "eject": "react-scripts eject",
    "eslint:check": "eslint --ext .jsx,.js src/",
    "eslint:write": "eslint --fix --ext .jsx,.js src/",
    "prettier:check": "prettier -c src/*",
    "prettier:write": "prettier -w src/*",
    "deploy": "gh-pages -d build -m '[ci skip] Updates'"
  },
  "dependencies": {
    "@polkadot/api": "^12.3.1",
    "@polkadot/extension-dapp": "^0.50.1",
    "@polkadot/keyring": "^13.0.2",
    "@polkadot/networks": "^13.0.2",
    "@polkadot/types": "^12.3.1",
    "@polkadot/ui-keyring": "^3.8.3",
    "@polkadot/ui-settings": "^3.8.3",
    "@polkadot/util": "^13.0.2",
    "@polkadot/util-crypto": "^13.0.2",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1",
    "semantic-ui-css": "Semantic-Org/Semantic-UI-CSS#master",
    "semantic-ui-react": "^2.1.5",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "gh-pages": "^6.1.1",
    "prettier": "3.3.3",
    "react-app-rewired": "^2.2.1",
    "semistandard": "^17.0.0"
  },
  "resolutions": {
    "eslint-plugin-jsx-a11y": "6.9.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "prettier"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.4.0"
}
